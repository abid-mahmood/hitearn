<% if flash[:notice].present? %>
  <div id="freeow-tr", class="freeow freeow-top-right">
    <div style="opacity: 1;">
      <div class="background">
        <div class="content">
        </div>
      </div>
    </div>
  </div>
  <input type='hidden', class='sync', value= "<%= raw flash[:notice] %>" />
<% end %>
<% if @confirm.present? %>
  <div class="progress empty" style="width: 230px; height: 25px; margin-left: 20px; margin-top: 10px;">
    You have clicked this Ad already.
<% else %>
  <div class="progress" style="width: 150px; height: 16px; margin-left: 20px; margin-top: 10px;" data-endpoint="<%= create_earning_user_earning_path(@adv.id) %>">
    <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
    0s
  </div>
<% end %>

</div>

<iframe src="<%= @adv.link.present? ? (@adv.link.starts_with?('http') || @adv.link.starts_with?('https') ? raw(@adv.link) : 'https://'+@adv.link ) : '' %>"></iframe>